% Please edit documentation in R/sde_core.R
\name{sde_mcmc_fit}
\alias{sde_mcmc_fit}
\title{Fit SDE parameters via MCMC (fmcmc)}
\usage{
sde_mcmc_fit(
  dat,
  initial = rep(0.01, 3),
  nsteps = 20000,
  prior_max = 100,
  kernel = fmcmc::kernel_ram()
)
}
\arguments{
\item{dat}{Data frame/matrix with columns: time, x1, x2.}

\item{initial}{Numeric vector length 3. Initial parameters c(r1, sigma1, c).}

\item{nsteps}{Integer. Number of MCMC steps.}

\item{prior_max}{Upper bound for Uniform prior on drift parameters.}

\item{kernel}{Proposal kernel from fmcmc, e.g., fmcmc::kernel_ram().}
}
\value{
MCMC chain matrix from fmcmc::MCMC().
}
\description{
Run MCMC for par = c(r1, sigma1, c) using the log posterior defined in
\code{sde_log_posterior()}. The time step dt is computed from the data.
}
